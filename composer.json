{
    "name": "ritaco/rita",
    "type": "cakephp-plugin",
    "description": "Rita Plugin is Rita App Core & magnements",
    "keywords": ["cakephp",  "Rita"],
    "homepage": "https://github.com/RitaCo/rita-core",
    "license": "MIT",
    "version": "3.0.0-dev",
    "authors": [
        {
            "name": "Saleh Souzanchi",
            "role": "Author",
            "homepage": "http://soozanchi.ir"
        }
    ],
    "require":{
        "php": ">=5.4",
		"cakephp/cakephp": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Rita\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rita\\Test\\": "tests"
        }
    },
    "support":{
        "source": "https://github.com/RitaCo/rita-core",
        "issues": "https://github.com/RitaCo/rita-core/issues"
    },
    "minimum-stability": "dev"
}
