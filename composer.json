{
	"name": "ritaco/rita",
	"type": "cakephp-plugin",
	"description": "This is base of Rita App Templates",
	"keywords": ["cakephp",  "Rita"],
	"homepage": "https://github.com/RitaCo/rita-core",
	"license": "MIT",
	"version": "0.1.0",
	"authors": [
		{
			"name": "Saleh Souzanchi",
			"role": "Author",
			"homepage": "http://www.ritaco.net"
		}
	],
	"require":{
		"php": ">=5.4",
        "cakephp/plugin-installer": "*",
		"cakephp/cakephp": "3.0.*-dev"
	},
	"autoload": {
		"psr-4": {
			"Rita\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Rita\\Test\\": "tests",
			"Cake\\Test\\Fixture\\": "vendor/cakephp/cakephp/tests/Fixture"
		}
	},
	"support":{
		"source": "https://github.com/RitaCo/rita-core",
		"issues": "https://github.com/RitaCo/rita-core/issues"
	},
	"extra": {
		"installer-name": "Rita"
	}
}
